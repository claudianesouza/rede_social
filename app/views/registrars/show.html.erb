<div class="page-header">
  <%= link_to registrars_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_registrar_path(@registrar), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show registrar</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nome:</dt>
  <dd><%= @registrar.nome %></dd>

  <dt>Sobrenome:</dt>
  <dd><%= @registrar.sobrenome %></dd>

  <dt>Email:</dt>
  <dd><%= @registrar.email %></dd>

  <dt>Senha:</dt>
  <dd><%= @registrar.senha %></dd>

</dl>
